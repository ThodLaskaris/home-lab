---
import '../styles/tailwind.css';
---
<main id="app">
	<header class="header">
		<h1 class="title">Manage House</h1>
		<p class="lead">Simple place to add and manage supermarket orders for your father.</p>
	</header>

	<section class="actions">
		<a href="/orders" class="btn">Open Orders</a>
		<a href="/new" class="btn muted">New Order</a>
	</section>

	<footer class="footer">
		<p class="small">Start editing src/pages or src/components to build your app.</p>
	</footer>
</main>
